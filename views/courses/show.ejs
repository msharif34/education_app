<% if(!currentUser){ %>
<div class="container">
  <h2> You must be a paid member to view this page.  Please register <a href="/auth/signup">here</a>, or <a href="/auth/login">login</a>.
</div>
<% }else{ %>
<div class="row">
  <br>
  <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
  </div>
  <div class="col-xs- col-sm- col-md- col-lg-">
    <div class="video">
      <iframe width="675" height="350" src="<%= course[0].link %>" frameborder="0" allowfullscreen></iframe>
    </div>
  </div>
  <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
  </div>
</div>
</div><!-- end slider -->
